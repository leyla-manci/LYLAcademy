{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "LYLAcademy-SPA": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/LYLAcademy-SPA",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [
              "src/styles.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "node_modules/bootswatch/dist/sketchy/bootstrap.min.css",
              "node_modules/alertifyjs/build/css/themes/bootstrap.min.css",
              "node_modules/alertifyjs/build/css/alertify.min.css",
              "node_modules/alertifyjs/build/css/themes/default.min.css",
              "src/vendor/datatables.net-bs4/css/dataTables.bootstrap4.css",
              "src/vendor/datatables.net-keytable-bs/css/keyTable.bootstrap.css",
              "src/vendor/datatables.net-responsive-bs/css/responsive.bootstrap.css",
              "src/vendor/@fortawesome/fontawesome-free/css/brands.css",
              "src/vendor/@fortawesome/fontawesome-free/css/regular.css",
              "src/vendor/@fortawesome/fontawesome-free/css/solid.css",
              "src/vendor/@fortawesome/fontawesome-free/css/fontawesome.css",
              "src/vendor/simple-line-icons/css/simple-line-icons.css",
              "src/vendor/animate.css/animate.css",
              "src/vendor/whirl/dist/whirl.css",
              "src/css/bootstrap.css",
              "src/css/app.css"
            ],
            "scripts": [
              "node_modules/alertifyjs/build/alertify.min.js",
              "src/vendor/modernizr/modernizr.custom.js",
              "src/vendor/js-storage/js.storage.js",
              "src/vendor/screenfull/dist/screenfull.js",
              "src/vendor/i18next/i18next.js",
              "src/vendor/i18next-xhr-backend/i18nextXHRBackend.js",
              "src/vendor/jquery/dist/jquery.js",
              "src/vendor/popper.js/dist/umd/popper.js",
              "src/vendor/bootstrap/dist/js/bootstrap.js",
              "src/vendor/datatables.net/js/jquery.dataTables.js",
              "src/vendor/datatables.net-bs4/js/dataTables.bootstrap4.js",
              "src/vendor/datatables.net-buttons/js/dataTables.buttons.js",
              "src/vendor/datatables.net-buttons-bs/js/buttons.bootstrap.js",
              "src/vendor/datatables.net-buttons/js/buttons.colVis.js",
              "src/vendor/datatables.net-buttons/js/buttons.flash.js",
              "src/vendor/datatables.net-buttons/js/buttons.html5.js",
              "src/vendor/datatables.net-buttons/js/buttons.print.js",
              "src/vendor/datatables.net-keytable/js/dataTables.keyTable.js",
              "src/vendor/datatables.net-responsive/js/dataTables.responsive.js",
              "src/vendor/datatables.net-responsive-bs/js/responsive.bootstrap.js",
              "src/vendor/jszip/dist/jszip.js",
              "src/vendor/pdfmake/build/pdfmake.js",
              "src/vendor/pdfmake/build/vfs_fonts.js",
              "src/js/app.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "LYLAcademy-SPA:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "LYLAcademy-SPA:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "LYLAcademy-SPA:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.css"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "LYLAcademy-SPA:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "LYLAcademy-SPA:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "LYLAcademy-SPA",
  "cli": {
    "analytics": false
  }
}
